name: dockercompose483394491638932896
services:
  msdb:
    container_name: watchshop-db
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Pavel0503!@#
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      shops: null
    ports:
    - mode: ingress
      target: 1433
      published: "8002"
      protocol: tcp
  watchshop.ui:
    build:
      context: E:\VSProjects\WatchShop
      dockerfile: WatchShop.UI/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WatchShop.UI
    container_name: WatchShop.UI
    depends_on:
      msdb:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
      DB_HOST: msdb
      DB_NAME: WatchStoreDB
      DB_SA_PASSWORD: Pavel0503!@#
    image: watchshopui
    labels:
      com.microsoft.visual-studio.project-name: WatchShop.UI
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/WatchShop.UI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      shops: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: C:\Users\trofimukpv\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\trofimukpv\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\trofimukpv\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
networks:
  shops:
    name: dockercompose483394491638932896_shops